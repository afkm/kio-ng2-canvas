{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "kio-ng2-canvas"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "test-app/assets",
        "test-app/favicon.ico"
      ],
      "index": "test-app/index.html",
      "main": "test-app/main.ts",
      "polyfills": "test-app/polyfills.ts",
      "test": "test-app/test.ts",
      "tsconfig": "test-app/tsconfig.app.json",
      "testTsconfig": "test-app/tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "./test-app/styles.scss"
      ],
      "environmentSource": "test-app/environments/environment.ts",
      "environments": {
        "dev": "test-app/environments/environment.ts",
        "prod": "test-app/environments/environment.prod.ts"
      }
    }
  ],
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
